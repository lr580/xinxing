.sele_range {
  position: absolute;
  top: 5px;
  left: 20px;
  background-color: wheat;
  color: black;
  z-index: 10;
}

.sele_block {
  position: absolute;
  top: 40px;
  left: 10px;
  background-color: wheat;
  height: 200px;
  z-index: 10;
}

.sele_skip{
  position: absolute;
  top: 20px;
  left: 260px;
  background-color: wheat;
  z-index: 10;
}

.on {
  display: inline;
}

.off {
  display: none;
}

.body-main-box {
  position: relative;
  overflow: hidden;
  height: 100vh;
}

.body-swiper {
  width: 680rpx;
  height: 900rpx;
  left: 50%;
  position: absolute;
  margin-left: -340rpx;
  z-index: 3;
  box-sizing: border-box;
  -webkit-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.62), 0 0 60px rgba(0, 0, 0, 0.06) inset;
  -moz-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.62), 0 0 40px rgba(0, 0, 0, 0.06) inset;
  box-shadow: 0 1px 4px rgba(0, 0, 0, 0.62), 0 0 40px rgba(0, 0, 0, 0.06) inset;
  border-radius: 12px;
}

