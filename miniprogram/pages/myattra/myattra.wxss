.contentbd{
  width: 92%;
  height: 120rpx;
  background-color: rgb(255, 255, 255);
  border-radius: 16rpx;
  display: flex;
  align-items: center;
  margin-left: 20rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.main
{
  display: flex;
  align-items: center;
}

.index1{
  height: 90rpx;
  font-size: 40rpx;
  font-weight: 200;
  padding: 20rpx;

  border-right: 8rpx solid #707070;
  display: flex;
  align-items: center;
}
.index2{
  height: 90rpx;
  font-size: 40rpx;
  font-weight: 200;
  padding: 20rpx;

  border-right: 8rpx solid #4ecdc4;
  display: flex;
  align-items: center;
}

.vname{
  font-weight:800 ;
  font-size: 40rpx;
  padding: 20rpx;
  color: #495060;
  width: 450rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}



.leftbd{
display: flex;
align-items: center;
}

.rightbd{
	border-top-left-radius: 16rpx;
  border-bottom-left-radius: 16rpx;
}

.rightbd image{
  width: 100rpx;
  height: 100rpx;
}

.haveicon{
  width: 120rpx;
  display: flex;
  align-items: center;
  height: 120rpx;
  justify-content: center;
}

.haveicon image{
  width: 50rpx;
  height: 25rpx;
}

.yes{
  width: 50rpx;
  height: 50rpx;
}

.no{
  width: 50rpx;
  height: 50rpx;
}

.limit{
  overflow:hidden; 
  white-space:nowrap;
}
